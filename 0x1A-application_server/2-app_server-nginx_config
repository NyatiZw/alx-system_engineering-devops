server {
	listen 5000 default_server;
	server_name _;

	location = /airbnb-onepage/ {
		proxy_pass http://0.0.0.0:5000/airbnb-onepage/;
	}

	add_header X-Served-By "190045-web-01";

	root /var/www/html;

	listen [::]:5000 default_server;

	index index.html index.htm index.nginx-dbian.html;

	location / {
		try_files $uri $uri/ =404;
	}
}
